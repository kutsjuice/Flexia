    "\n",
    "# Rotation matrices\n",
    "Ri = [cos(_θi) -sin(_θi); sin(_θi) cos(_θi)]\n",
    "Rj = [cos(_θj) -sin(_θj); sin(_θj) cos(_θj)]\n",
    "\n",
    "# Local vectors\n",
    "pci = [xci; yci]\n",
    "pcj = [xcj; ycj]\n",
    "ni = [-ydi; xdi]  # Normal to body i's direction\n",
    "# nj = [-ydj; xdj]  # Normal to body j's direction\n",
    "\n",
    "# Global positions\n",
    "pbi = [_xi; _yi]\n",
    "pbj = [_xj; _yj]\n",
    "\n",
    "# Global joint points\n",
    "p_joint_i = pbi + Ri * pci\n",
    "p_joint_j = pbj + Rj * pcj\n",
    "\n",
    "# Global normal from body i (reference normal for translational constraint)\n",
    "n_global_i = Ri * ni\n",
    "\n",
    "# Constraint equations\n",
